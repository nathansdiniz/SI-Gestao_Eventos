  ddiTelefone2        String?   // DDI do telefone secundário (ex: (31))
  telefone2           String?   // Telefone secundário
  ddiCelular          String?   // DDI do celular (ex: (31))
  celular             String?   // Celular
  redeSocial          String?   // URL da rede social

  pessoaFisica      PessoaFisica?
  pessoaJuridica    PessoaJuridica?
   id_empresa                Int?     
}
model PessoaFisica {
  id                Int      @id @default(autoincrement())
