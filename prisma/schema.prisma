}
model Cliente {
  id                Int      @id @default(autoincrement())
  tipoCadastro      Boolean  // false = PF, true = PJ
  nome              String
  email             String?
  telefone          String?
  endereco          String?
  cep               String?
  complemento       String?
  cidade            String?
